<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_4_0.xsd" version="2.5">

    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>register.servlets.Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/Register/test</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ConsultServlet</servlet-name>
        <servlet-class>consult.servlets.Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ConsultServlet</servlet-name>
        <url-pattern>/Consult/servlet</url-pattern>
    </servlet-mapping>
</web-app>
